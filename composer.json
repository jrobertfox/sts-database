{
   "repositories":[
      {
         "type":"git",
         "url":"https://github.com/jrobertfox/zend-form-decorators-bootstrap.git"
      }
   ],
   "require":{
      "mandrill/mandrill":"*",
      "behat/behat":"*",
      "behat/mink":"*",
      "behat/mink-extension":"*",
      "behat/mink-goutte-driver":"1.0.3",
      "breerly/zf1":"1.11.*",
      "phpunit/phpunit":"*",
      "emagister/zend-form-decorators-bootstrap":"*",
      "mockery/mockery": "*"
   },
   "minimum-stability":"dev",
   "config":{
      "bin-dir":"bin/"
   },
   "autoload":{
      "psr-0":{
         "STS":["application/src","application/tests/unit","application/tests/integration"]
      }
   }
}
